<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="j.jave.framework.components.login.mapper.UserTrackerMapper" >

<resultMap id="base"  type="j.jave.framework.model.JBaseModel" >
		<result column="ID" property="id" />
		<result column="CREATEID" property="createId" />
		<result column="UPDATEID" property="updateId" />
		<result column="CREATETIME" property="createTime" />
		<result column="UPDATETIME" property="updateTime" />
		<result column="DELETED" property="deleted" />
		<result column="VERSION" property="version" />
</resultMap>

<resultMap type="j.jave.framework.components.login.model.UserTracker" id="userTracker"  extends="base" >
	<result column="USERID" property="userId" />
	<result column="USERNAME" property="userName" />
	<result column="IP" property="ip" />
	<result column="LOGIN_TIME" property="loginTime" />
	<result column="LOGIN_CLIENT" property="loginClient" />
</resultMap>


<insert  id="save" parameterType="j.jave.framework.components.login.model.UserTracker">
insert into USER_TRACKER
(
ID,CREATEID,UPDATEID,CREATETIME,UPDATETIME,DELETED,VERSION
,USERID
,USERNAME
,IP
,LOGIN_TIME
,LOGIN_CLIENT
)
 values 
(
#{id}
,#{createId}
,#{updateId}
,#{createTime}
,#{updateTime}
,#{deleted}
,#{version}
,#{userId}
,#{userName}
,#{ip}
,#{loginTime}
,#{loginClient}
) 
</insert>

<select id="get"   resultMap="userTracker"  >
select * from USER_TRACKER where id=#{param1}
</select>

<select id="getUserTrackerByName"  resultMap="userTracker"  >
select * from USER_TRACKER where DELETED='N' and USERNAME=#{userName} order by LOGIN_TIME desc
</select>





</mapper>