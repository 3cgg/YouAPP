<?xml version="1.0"?>
<configuration>

<!-- xmldb properties  -->

<property>
  <name>youapp.atomic.resource.session.provider</name>
  <value>j.jave.kernal.jave.xml.xmldb.JXMLSessionProvider</value>
  <description>xml session provider</description>
</property>

<property>
  <name>youapp.xmldb.url</name>
  <value>file:/D:/XML-DATA</value>
  <description>where the xml files store all data.</description>
</property>

<property>
  <name>youapp.security.desede.key</name>
  <value>youapp.security.desede.key.jia.zhong.jin</value>
  <description>the key used to be for DES cipher.</description>
</property>

<property>
  <name>youapp.json.default.date.format</name>
  <value>yyyy-MM-dd HH:mm:ss</value>
  <description>the default json format.</description>
</property>

<property>
  <name>youapp.http.factory</name>
  <value>j.jave.kernal.http.JApacheHttpClientFactory</value>
  <description>Apache http-client implement.</description>
</property>

<property>
  <name>youapp.base64.factory</name>
  <value>j.jave.kernal.jave.base64.JApachBase64Factory</value>
  <description>Apache base64 implement.</description>
</property>

<property>
  <name>youapp.logger.factory</name>
  <value>j.jave.kernal.jave.logging.JSLF4JLoggerFactory</value>
  <description>Slf4j logging API.</description>
</property>

<property>
  <name>youapp.services</name>
  <value></value>
  <description>all registered services  in the service hub.
  	{test.j.jave.kernal.eventdriven.TestUserService=test.j.jave.kernal.eventdriven.TestUserService;
  	test.j.jave.kernal.eventdriven.TestUserService=test.j.jave.kernal.eventdriven.TestUserService;
  	test.j.jave.kernal.eventdriven.TestUserService=test.j.jave.kernal.eventdriven.TestUserService}
  </description>
</property>


<property>
  <name>youapp.event.pipe.prefix.logging.class</name>
  <value>j.jave.kernal.eventdriven.servicehub.JEventQueueLoggingPipe</value>
  <description>Event logging API.</description>
</property>

<property>
  <name>youapp.event.pipe.prefix.logging.order</name>
  <value>2</value>
  <description>Event logging API.</description>
</property>

<property>
  <name>youapp.event.pipe.prefix.logging.message</name>
  <value>Log all message.</value>
  <description>Event logging API.</description>
</property>


<property>
  <name>youapp.service.async.event.result.repo</name>
  <value>JDefaultAsyncEventResultRepoService</value>
  <description>Async Event Result Repo Service</description>
</property>

</configuration>
