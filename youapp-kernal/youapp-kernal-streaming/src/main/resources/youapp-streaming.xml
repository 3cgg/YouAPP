<?xml version="1.0"?>
<configuration>

<!-- xmldb properties  -->

<property>
  <name>youapp.streaming.zookeeper.server</name>
  <value>nim1.storm.com:2182,nim2.storm.com</value>
  <description>zookeeper server</description>
</property>

<property>
  <name>youapp.streaming.zookeeper.namespace</name>
  <value>z-test</value>
  <description>zookeeper namespace</description>
</property>

<property>
  <name>youapp.streaming.netty.server.port</name>
  <value>8080</value>
  <description>leader always listener on the port</description>
</property>

<property>
  <name>youapp.streaming.node.id</name>
  <value>1</value>
  <description>node id</description>
</property>

<property>
  <name>youapp.streaming.node.name</name>
  <value></value>
  <description>node name</description>
</property>

<property>
  <name>youapp.streaming.workflow.tracking.kafka.topic</name>
  <value>workflow-instance-track</value>
  <description>kafka</description>
</property>

<property>
  <name>youapp.streaming.rpc.host.znode</name>
  <value>/leader-host</value>
  <description>zookeeper node from which the remote host can be found</description>
</property>


</configuration>
